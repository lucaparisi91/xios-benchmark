<context>
  <variable_definition>
    <variable_group id="buffer">
      <variable id="optimal_buffer_size" type="string">
	performance
      </variable>
      <variable id="buffer_size_factor" type="double"> 1.0 </variable>
      <variable id="min_buffer_size" type="int">2097152 </variable>
    </variable_group>
    
    <variable_group id="parameters" >
       <variable id="using_server" type="bool">true</variable>
        <variable id="using_server2" type="bool">false</variable>
       <variable id = "services_use_window_manager" type="bool">false</variable> 
        <variable id="transport_protocol" type="string" >p2p</variable> 
       <variable id="info_level" type="int"> 100 </variable>
      <variable id="print_file" type="bool"> true </variable>
    </variable_group>
  </variable_definition>
</context>


