<context>
  <calendar type="Gregorian"/>
    <axis_definition>
      <axis id="pressure_levels1" positive="down" long_name="Air Pressure" standard_name="air_pressure" unit="Pa" n_glo="10" >
      </axis>
    </axis_definition>
    
    <grid_definition>
      <grid id="model">
        <axis axis_ref="pressure_levels1" />
      </grid>
    </grid_definition>
    
    <field_definition freq_op="1ts"  enabled=".TRUE." operation="instant" >
      <field id="pressure" name="pressure" long_name="Air Pressure" standard_name="air_pressure" unit="Pa" grid_ref="model" />
    </field_definition>
  
    
  <file_definition type="one_file" >
    <file id="axis_output" name="axis" >

    </file> 
  </file_definition>
  
</context>